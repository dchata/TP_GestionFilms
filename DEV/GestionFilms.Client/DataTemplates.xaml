<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:GestionFilms.Client"
                    xmlns:views="clr-namespace:GestionFilms.Client.Views"
                    xmlns:viewmodels="clr-namespace:GestionFilms.Client.ViewModels">

  <DataTemplate DataType="{x:Type viewmodels:ViewModelHome}" x:Key="ViewModelHomeTemplate">
    <views:Home/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type viewmodels:ViewModelFilms}" x:Key="ViewModelFilmsTemplate">
    <views:ListFilms/>
  </DataTemplate>
  
  <DataTemplate DataType="{x:Type viewmodels:ViewModelDetailsFilm}" x:Key="ViewModelDetailsFilmTemplate">
    <views:DetailsFilm/>
  </DataTemplate>

  <DataTemplate DataType="{x:Type viewmodels:ViewModelFilmGenre}" x:Key="ViewModelFilmGenreTemplate">
    <views:ListFilmGenres/>
  </DataTemplate>


</ResourceDictionary>